    <DockPanel Background="#BBEEEEEE">
        <Grid DockPanel.Dock="Top" Height="48"
            Background="Transparent"
            PointerPressed="TitleBar_PointerPressed"
            ColumnDefinitions="Auto,1*,Auto">
            <Image Grid.Column="0"
                 Source="/Assets/avalonia-logo.ico"
          Width="24" Height="24"
          Margin="12,0,8,0"
          VerticalAlignment="Center"
          Opacity="0.9"/>
            <!-- 标题文本 -->
            <TextBlock Grid.Column="1"
                     Classes="title-text"
                     Text="{x:Static lang:Resources.MainMenuTitle}"
                     VerticalAlignment="Center"/>
            <!-- 窗口控制按钮 -->
            <StackPanel Grid.Column="3" Orientation="Horizontal">
                <u:Breadcrumb Height="18"  Margin="0 0 40 0" Padding="8">
                    <u:IconButton Classes="breadcrumb-bar-btn" Icon="{StaticResource github_regular}" Click="GithubButton_Click"/>
                    <u:IconButton Classes="breadcrumb-bar-btn" Icon="{StaticResource local_language_regular}">
                        <u:IconButton.Flyout>
                            <MenuFlyout>
                                <MenuItem Command="{CompiledBinding ChangeLanguageCommand}" CommandParameter="zh-CN">
                                    <MenuItem.Header>
                                        <DockPanel>
                                            <PathIcon Data="{StaticResource lang_cn}" Margin="0 0 4 0"/>
                                            <Label Content="{x:Static lang:Resources.Lang_CN}"/>
                                        </DockPanel>
                                    </MenuItem.Header>
                                </MenuItem>
                                <MenuItem Command="{CompiledBinding ChangeLanguageCommand}" CommandParameter="en-US">
                                    <MenuItem.Header>
                                        <DockPanel>
                                            <PathIcon Data="{StaticResource lang_us}" Margin="0 0 4 0"/>
                                            <Label Content="{x:Static lang:Resources.Lang_US}"/>
                                        </DockPanel>
                                    </MenuItem.Header>
                                </MenuItem>
                            </MenuFlyout>
                        </u:IconButton.Flyout>
                    </u:IconButton>
                </u:Breadcrumb>
                <u:IconButton
                    Classes="title-bar-btn"
                    Click="MinimizeButton_Click"
                    Icon="{StaticResource arrow_minimize_regular}"
                    Margin="0 0 8 0"
                    />
                <u:IconButton
                    Classes="title-bar-btn close"
                    Click="CloseButton_Click"
                    Icon="{StaticResource close_regular}"
                    Margin="0 0 10 0"
                    />
            </StackPanel>
        </Grid>
        <Grid Margin="20" Background="#CCFFFFFF">
            <views:MainView />
        </Grid>
    </DockPanel>
</Window>