		BEGIN
			VALUE "FileDescription", "MetaHook Plus"
			VALUE "FileVersion", "0.4.0.0"
			VALUE "InternalName", "MetaHook"
			VALUE "OriginalFilename", "MetaHook.exe"
		END
	END

	BLOCK "VarFileInfo"

	BEGIN
		VALUE "Translation", 0x0, 1200
	END
END